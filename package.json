{
  "name": "vscode-package-version",
  "displayName": "vscode-package-version",
  "description": "Show all package dependencies' latest version",
  "publisher": "talljack",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "main": "./dist/index.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-package-version.init",
        "title": "Find Package latest version",
        "enablement": "resourceFilename == package.json"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup ./src/index.ts --external vscode",
    "vscode:prepublish": "pnpm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.23.0",
    "eslint": "^8.56.0",
    "eslint-plugin-json": "^3.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
